{
  "name": "Digilender",
  "version": "1.0.0",
  "description": "A digital calendar for the family",
  "main": "main.js",
  "scripts": {
    "start": "electron --disable-gpu .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . digilender --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Digilender\"",
    "package-linux": "electron-packager . digilender --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Digilender Team",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-packager": "^12.2.0"
  }
}